<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Fee_Paid_Validation</fullName>
    <active>true</active>
    <description>Fee Paid cannot exceed Total Fee</description>
    <errorConditionFormula>Fee_Paid__c &gt; Total_Fee__c</errorConditionFormula>
    <errorDisplayField>Fee_Paid__c</errorDisplayField>
    <errorMessage>Fee Paid cannot be more than Total Fee</errorMessage>
</ValidationRule>
